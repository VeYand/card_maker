{
  "env": {
    "browser": true,
    "es2021": true
  },
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "tsx": true
    }
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-duplicate-imports": "error",
    "no-self-compare": "error",
    "no-use-before-define": "error"
  }
}
